{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "LocalDB": "Server=(localdb)\\MSSQLLocalDB;Database=Ebay;Trusted_Connection=True;MultipleActiveResultSets=true",
    "AzureSql": "ConnectionStringToBeAdded"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "": null,
          "ConnectionStrings": {
            "LocalDB": "Server=(localdb)\\MSSQLLocalDB;Database=Ebay;Trusted_Connection=True;MultipleActiveResultSets=true",
            "AzureSql": "ConnectionStringToBeAdded"
          },
          "tableName": "Logs",
          "autoCreateSqlTable": true
        }
      },
      {
        "Name": "Console"
      }
    ]
  }
}


